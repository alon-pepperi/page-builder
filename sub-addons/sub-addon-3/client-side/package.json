{
  "name": "client-side",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload=false --sourceMap=true",
    "start:sub-addon": "ng serve sub-addon --live-reload false",
    "single-spa": "ng serve --port 4400 --configuration SingleSpa --sourceMap=false --live-reload=false",
    "standalone": "ng serve --port 4400 --configuration Standalone --sourceMap=false --live-reload=false",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:single-spa": "ng build --prod --sourceMap=false",
    "build:single-spa:client-side": "ng build  --prod",
    "serve:single-spa:client-side": "ng s --project client-side --disable-host-check --port 4400 --deploy-url http://localhost:4400/ --live-reload false",
    "assets-server": "http-server ./src/ -p 4300 -d --cors"
  },
  "private": true,
  "dependencies": {
